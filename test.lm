void main() {
    str greeting = "Hello, world!"
    int x = 5
    int y = 10

    print("{greeting}")
    print("The sum of {x} and {y} is {add(x, y)}")

    if multiply(3, 3) > add(3, 3) && true == true {
        print("3*3 is greater than 3+3")
    } else if 1==1 {
        print("else if case")
    } else {
        print("else case")
    }

    while true {
        print("This is a forever loop")
    }
}

int add(int a, int b) {
    return a + b
}

int multiply(int a, int b) {
    return a * b
}
